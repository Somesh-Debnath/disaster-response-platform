							"raw": "{{baseUrl}}/api/v1/disasters/{{disasterId}}/social-media/refresh",
							"host": ["{{baseUrl}}"],
							"path": ["api", "v1", "disasters", "{{disasterId}}", "social-media", "refresh"]
						}
					}
				}
			]
		},
